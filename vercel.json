{
    "version": 2,
    "builds": [
      {
        "src": "stock-tracker/manage.py",
        "use": "@vercel/python",
        "config": {
          "Pipfile": "stock-tracker/Pipfile"
        }
      }
    ],
    "routes": [
        { "handle": "filesystem" },
        { "src": "/stock-tracker/(.*)", "dest": "/stock-tracker/$1" }
    ]
  }
  